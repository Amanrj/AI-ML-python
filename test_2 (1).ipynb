    "def fibonacci(n):\n",
    "    if n <= 0:\n",
    "        return []\n",
    "    elif n == 1:\n",
    "        return [0]\n",
    "    elif n == 2:\n",
    "        return [0, 1]\n",
    "\n",
    "    sequence = [0, 1]\n",
    "    for i in range(2, n):\n",
    "        sequence.append(sequence[-1] + sequence[-2])\n",
    "    \n",
    "    return sequence\n",
    "\n",
    
    "n = 10\n",
    "result = fibonacci(n)\n",
    "print(f\"The first {n} numbers in the Fibonacci sequence are: {result}\")\n"
   ]
